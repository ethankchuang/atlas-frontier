# Database Configuration
SUPABASE_URL=your_supabase_project_url
SUPABASE_ANON_KEY=your_supabase_anon_key
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key
SUPABASE_JWT_SECRET=your_supabase_jwt_secret

# Redis Configuration (Railway will provide these automatically if you add Redis service)
REDIS_URL=redis://localhost:6379

# AI API Keys
OPENAI_API_KEY=your_openai_api_key
REPLICATE_API_TOKEN=your_replicate_api_token

# JWT Configuration
SECRET_KEY=your_super_secret_jwt_key_change_this_in_production

# Server Configuration
HOST=0.0.0.0
PORT=8000
DEBUG=false

# Game Settings
DEFAULT_WORLD_SEED=fantasy_world_v1
MAX_PLAYERS_PER_ROOM=10
IMAGE_GENERATION_ENABLED=true
IMAGE_PROVIDER=replicate

# Replicate Settings
REPLICATE_MODEL=black-forest-labs/flux-schnell
REPLICATE_IMAGE_WIDTH=1024
REPLICATE_IMAGE_HEIGHT=576

# Database Settings
CHROMA_PERSIST_DIRECTORY=./data/chroma
CHROMA_ALLOW_RESET=false
CHROMA_TELEMETRY_ENABLED=false

# Security
ACCESS_TOKEN_EXPIRE_MINUTES=60
